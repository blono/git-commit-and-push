{
    "name": "git-commit-and-push",
    "displayName": "Git Commit & Push",
    "description": "Do git commit and push.",
    "icon": "images/icon.png",
    "version": "0.0.3",
    "publisher": "blono",
    "repository": {
        "type": "git",
        "url": "https://github.com/blono/git-commit-and-push"
    },
    "engines": {
        "vscode": "^1.55.0"
    },
    "license": "MIT",
    "categories": [
        "Other"
    ],
    "keywords": [
        "git",
        "commit",
        "push",
        "sync"
    ],
    "activationEvents": [
        "onCommand:git-commit-and-push.commitAndPush"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "git-commit-and-push.commitAndPush",
                "title": "%git-commit-and-push.title%",
                "category": "Git",
                "icon": "$(repo-force-push)"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "git-commit-and-push.commitAndPush",
                    "when": "config.git.enabled && !git.missing && gitOpenRepositoryCount != 0"
                }
            ],
            "scm/title": [
                {
                    "command": "git-commit-and-push.commitAndPush",
                    "group": "navigation@1",
                    "when": "scmProvider == git"
                },
                {
                    "command": "git-commit-and-push.commitAndPush",
                    "group": "inline",
                    "when": "scmProvider == git"
                }
            ]
        }
    },
    "scripts": {
        "watch": "tsc -b -w",
        "compile": "tsc -b",
        "clean": "gulp clean"
    },
    "devDependencies": {
        "@types/node": "14.x",
        "@types/vscode": "^1.55.0",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "eslint": "^8.1.0",
        "event-stream": "^4.0.1",
        "del": "^6.0.0",
        "gulp": "^4.0.2",
        "gulp-filter": "^7.0.0",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-typescript": "^5.0.1",
        "run-sequence": "^2.2.1",
        "typescript": "^4.4.4",
        "vsce": "^2.4.0",
        "vscode-nls-dev": "^3.3.2"
    },
    "dependencies": {
        "vscode-nls": "^5.0.0"
    }
}
